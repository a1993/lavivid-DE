{% assign overview = product.metafields.custom.product_overview %}

  {% if overview != blank and overview.value != blank and section.blocks.size > 0 %}
    <section class="looks-details-section-{{ section.id }}">
        <div class="container">
            <div class="row">
                <div class="look-details-heading-wrapper text-center">
                    {% if section.settings.heading_text_1 != blank or section.settings.heading_text_2 != blank %}
      <h2 class="content-title look-details-heading">
        <span class="look-details-heading-blue" style="color: {{ section.settings.heading_color_1 }}">
          {{ section.settings.heading_text_1 }}
        </span>
        <span class="custom-heading__secondary" style="color: {{ section.settings.heading_color_2 }}">
          {{ section.settings.heading_text_2 }}
        </span>
      </h2>
    {% endif %}
    {% if section.settings.paragraph !=blank %}
                    <div class="look-details-text">
            {{section.settings.paragraph}}
            </div>
          {% endif %}
                </div>
                {% if section.blocks.size < 4 %}
                <div class="looks-details-static">
                    {% for block in section.blocks %}
                    <div class="looks-details-card">
        <div class="looks-details-card-img">
          {% if block.settings.look_detail_image != blank %}
            <img src="{{ block.settings.look_detail_image | img_url: 'master' }}" alt="{{ block.settings.look_detail_title }}">
          {% endif %}
        </div>
        <div class="looks-details-card-content">
                    {% if block.settings.look_detail_title != blank %}
          <div class="looks-details-card-title"><h3>{{ block.settings.look_detail_title }}</h3></div>
                    {% endif %}
                    {% if block.settings.look_detail_text != blank %}
          <div class="looks-details-card-text">{{ block.settings.look_detail_text }}</div>
                    {% endif %}
        </div>
      </div>
    {% endfor %}
                </div>
                {% else %}
                <div class="looks-details look-details-slider">
                    <div class="swiper-wrapper looks-details-card-wrapper">
                         {% for block in section.blocks %}
             <div class="swiper-slide looks-details-card-slide">
                       <div class="looks-details-card">
        <div class="looks-details-card-img">
          {% if block.settings.look_detail_image != blank %}
            <img src="{{ block.settings.look_detail_image | img_url: 'master' }}" alt="{{ block.settings.look_detail_title }}">
          {% endif %}
        </div>
        <div class="looks-details-card-content">
                    {% if block.settings.look_detail_title != blank %}
          <div class="looks-details-card-title"><h3>{{ block.settings.look_detail_title }}</h3></div>
                    {% endif %}
                    {% if block.settings.look_detail_text != blank %}
          <div class="looks-details-card-text">{{ block.settings.look_detail_text }}</div>
                    {% endif %}
        </div>
      </div>
    </div>
  {% endfor %}
  </div>
          <div class="looks-details-button-next">
