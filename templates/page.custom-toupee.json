        "html_xdAdwL": {
          "type": "html",
          "settings": {
            "html_content": "<script>\n\n\n  function loadScriptReliably(src, callback) {\n      // 创建 script 元素\n      var script = document.createElement('script');\n      script.src = src;\n      script.async = true;\n\n      // 标记是否已完成\n      var done = false;\n\n      // 成功处理函数\n      function success() {\n          if (done) return;\n          done = true;\n          callback(true);\n      }\n\n      // 失败处理函数\n      function fail() {\n          if (done) return;\n          done = true;\n          callback(false);\n      }\n\n      // 绑定所有可能的事件\n      script.onload = success;\n      script.onreadystatechange = function () {\n          if (this.readyState === 'loaded' || this.readyState === 'complete') {\n              success();\n          }\n      };\n      script.onerror = fail;\n\n      // 添加到文档\n      document.head.appendChild(script);\n\n      // 超时检查\n      setTimeout(function () {\n          if (!done) {\n              // 检查特定功能是否存在来判断是否加载成功\n              if (src.includes('richUI') && typeof jQuery !== 'undefined' && jQuery.fn.richUI) {\n                  success();\n              } else {\n                  fail();\n              }\n          }\n      }, 3000);\n  }\n\n  // 在顶层作用域声明变量，使其在整个 script 内可访问\n  var dataId, dataEmail, dataFirstname, dataLastname, urlTokens, encryptedText, encodedURL, baseUrl, script;\n\n  function waitForJQuery() {\n      return new Promise((resolve) => {\n          if (window.jQuery) {\n              resolve();\n          } else {\n              const checkInterval = setInterval(() => {\n                  if (window.jQuery) {\n                      clearInterval(checkInterval);\n                      resolve();\n                  }\n              }, 100);\n          }\n      });\n  }\n\n  // 等待 jQuery 加载完成，然后等待 DOM 就绪\n  waitForJQuery().then(() => {\n\n      // 动态创建并加载 richUI 脚本\n\n\n\n      localStorage.setItem('$(\".eml\").text()', $(\".eml\").text())\n      dataId = $(\".id\").text();\n      dataEmail = $(\".toupee-email\").text();\n      dataFirstname = $(\".firstname\").text();\n      dataLastname = $(\".lastname\").text();\n      urlTokens = dataId + '###' + dataEmail + '###' + dataFirstname + '###' + dataLastname;\n      encryptedText = encryptWithAES(urlTokens, \"ajxkidjdfsioufxoivsdxzffjsdioo32\");\n      encodedURL = encodeURIComponent(encryptedText);\n      var elem = document.getElementById(\"elem\");\n      console.log('dataEmail', dataEmail);\n      if (dataEmail !== \"\") {\n          elem.style.display = \"none\";\n      }\n      console.log(\"customizecustomize\", customize);\n      function encryptWithAES(text, secretKey) {\n          // 将密钥转换为与 Java 中相同长度的字节\n          var key = CryptoJS.enc.Utf8.parse(secretKey);\n\n          // 进行 AES 加密\n          var encrypted = CryptoJS.AES.encrypt(text, key, {\n              mode: CryptoJS.mode.ECB,\n              padding: CryptoJS.pad.Pkcs7\n          });\n\n          // 返回 Base64 编码的字符串\n          return encrypted.toString();\n      }\n      baseUrl = 'https://customization.lavividhair.com/lvGood';\n      $(\".beiZhuBox\").hide();\n      //浮窗\n      var currentIndex = 0;\n\n      // 点击.tab-li元素\n      $(\".tab-li\").click(function () {\n          var _this = $(this);\n          var _thisIndex = _this.index();\n\n          // 更新currentIndex\n          currentIndex = _thisIndex;\n\n          _this.addClass(\"active\").siblings(\".tab-li\").removeClass(\"active\");\n          $(\".tabList-li\").eq(_thisIndex).fadeIn().siblings('.tabList-li').fadeOut();\n\n          updateButtons();\n      });\n\n      // 上一步按钮\n      $(\"#prevBtn\").click(function () {\n          if (currentIndex > 0) {\n              currentIndex--;\n              $(\".tab-li\").eq(currentIndex).trigger(\"click\");\n          }\n      });\n\n      // 下一步按钮\n      $(\"#nextBtn\").click(function () {\n          var tabCount = $(\".tab-li\").length;\n          if (currentIndex < tabCount - 1) {\n              currentIndex++;\n              $(\".tab-li\").eq(currentIndex).trigger(\"click\");\n          }\n      });\n\n      /*-- 2024.04.17 --*/\n\n      // 触发下一个选项卡的显示\n      $(\"input[type='radio']\").click(function () {\n          if ($(window).width() > 767) {\n              $(\"#nextBtn\").trigger(\"click\");\n          }\n      });\n      $(\" dt img\").click(function () {\n          var parentDL = $(this).closest(\"dl\");\n          var radioButtonId = parentDL.find(\"input[type='radio']\").attr(\"id\");\n          // 触发对应单选按钮的点击事件\n          $(\"#\" + radioButtonId).click();\n      });\n      $(document).ready(function () {\n\n          if ($(window).width() < 767) {\n              let editBtnVisible = true;\n\n              function updateOptions(optionClass, selectedBtn) {\n                  $(optionClass).hide();\n                  selectedBtn.closest('.color-radio').find('.color-option dl').hide();\n                  selectedBtn.closest('dl').show();\n                  selectedBtn.closest('.color-option').show();\n                  if (editBtnVisible) {\n                      selectedBtn.closest('.base-radio, .color-radio, .wave-radio').find('.edit-btn').show().siblings('.edit-btn').hide();\n                  } else {\n                      selectedBtn.closest('.base-radio, .color-radio, .wave-radio').find('.edit-btn').hide();\n                  }\n              }\n\n              $('.base-radio input[type=\"radio\"]').change(function () {\n                  updateOptions('.base-option', $(this));\n              });\n\n              $('.color-radio input[type=\"radio\"]').change(function () {\n                  updateOptions('.color-option', $(this));\n              });\n\n              $('.wave-radio input[type=\"radio\"]').change(function () {\n                  updateOptions('.wave-option', $(this));\n              });\n\n              $('.edit-btn').click(function () {\n                  $(this).siblings('.edit-btn').show();\n                  $(this).hide();\n                  $(this).siblings('.base-option, .color-option, .wave-option').show();\n                  $(this).closest('.color-radio').find('.color-option dl').show();\n                  editBtnVisible = !editBtnVisible;\n              });\n\n              const grayHairBox = $('.grayHairBox');\n              const editBtn = $('.edit-btn');\n              const selectedValueContainer = $('.selected-value');\n\n              function showSelectedValue() {\n                  const selectedRadio = $('input[name=\"gray-hair\"]:checked');\n                  if (selectedRadio.length > 0) {\n                      selectedValueContainer.text(selectedRadio.attr('lab'));\n                      grayHairBox.addClass('hidden');\n                      selectedValueContainer.parent().removeClass('hidden');\n                      editBtn.show(); // 修正为显示按钮\n                      editBtnVisible = true; // 重新设置 editBtnVisible\n                      $(\"#submitForm\").removeAttr(\"disabled\").removeAttr(\"style\");\n\n                  }\n              }\n\n              function showGrayHairBox() {\n                  grayHairBox.removeClass('hidden');\n                  selectedValueContainer.parent().addClass('hidden');\n                  editBtnVisible = true;\n                  $(\"#submitForm\").removeAttr(\"disabled\").removeAttr(\"style\");\n\n              }\n\n              editBtn.on('click', showGrayHairBox);\n              $('input[name=\"gray-hair\"]').on('change', showSelectedValue);\n\n          }\n      });\n\n\n\n      /*-- end 2024.04.17 --*/\n\n\n      function updateButtons() {\n          if (currentIndex === 0) {\n              $(\"#prevBtn\").hide();\n          } else {\n              $(\"#prevBtn\").show();\n          }\n\n          if (currentIndex === $(\".tab-li\").length - 1) {\n              $(\"#nextBtn\").hide();\n          } else {\n              $(\"#nextBtn\").show();\n          }\n      }\n      $(function () {\n          // 新增 - tab切换\n          $(\".tab-nav\").each(function () {\n\n             