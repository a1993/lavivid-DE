          "type": "html",
          "settings": {
            "html_content": "<script>\n// 立即定义全局函数 - 在HTML加载前执行\nwindow.openAppointmentModal = function() {\n  console.log('openAppointmentModal 被调用');\n  const modal = document.getElementById('appointmentModal');\n  if (modal) {\n    modal.classList.add('active');\n    document.body.style.overflow = 'hidden';\n    console.log('✓ 弹框已打开');\n  } else {\n    console.error('✗ 找不到弹框元素 #appointmentModal');\n  }\n};\n\nwindow.closeAppointmentModal = function() {\n  console.log('closeAppointmentModal 被调用');\n  const modal = document.getElementById('appointmentModal');\n  if (modal) {\n    modal.classList.remove('active');\n    document.body.style.overflow = 'auto';\n    console.log('✓ 弹框已关闭');\n  }\n};\n\nconsole.log('✓ 全局函数已定义');\nconsole.log('✓ window.openAppointmentModal:', typeof window.openAppointmentModal);\nconsole.log('✓ window.closeAppointmentModal:', typeof window.closeAppointmentModal);\n</script>\n\n<!-- 弹框HTML结构 -->\n<div id=\"appointmentModal\" class=\"appointment-modal\">\n  <div class=\"appointment-modal-content\">\n    <button class=\"appointment-modal-close\" aria-label=\"关闭弹框\" type=\"button\" onclick=\"window.closeAppointmentModal(); return false;\">×</button>\n    <iframe class=\"appointment-modal-iframe\" src=\"https://lavividhair-gmbh.salonized.com/\" title=\"预约系统\" allow=\"camera; microphone\"></iframe>\n  </div>\n</div>\n\n<script>\n// 初始化事件监听器 - 在HTML加载后执行\nfunction initAppointmentModal() {\n  const modal = document.getElementById('appointmentModal');\n  const closeBtn = document.querySelector('.appointment-modal-close');\n  const appointmentButtons = document.querySelectorAll('[data-appointment-trigger]');\n  \n  console.log('=== 预约系统初始化 ===');\n  console.log('✓ 弹框元素:', !!modal);\n  console.log('✓ 关闭按钮:', !!closeBtn);\n  console.log('✓ 预约按钮数量:', appointmentButtons.length);\n  \n  if (!modal) {\n    console.error('✗ 错误: 找不到弹框元素 #appointmentModal');\n    return;\n  }\n  \n  // 绑定预约按钮点击事件（备用方案）\n  appointmentButtons.forEach((btn, index) => {\n    btn.addEventListener('click', function(e) {\n      e.preventDefault();\n      e.stopPropagation();\n      console.log('✓ 预约按钮 #' + (index + 1) + ' 被点击（事件监听器）');\n      window.openAppointmentModal();\n    });\n    console.log('✓ 已绑定预约按钮 #' + (index + 1) + ' 的点击事件');\n  });\n  \n  // 绑定关闭按钮点击事件（备用方案）\n  if (closeBtn) {\n    closeBtn.addEventListener('click', function(e) {\n      e.preventDefault();\n      e.stopPropagation();\n      console.log('✓ 关闭按钮被点击（事件监听器）');\n      window.closeAppointmentModal();\n    });\n    console.log('✓ 已绑定关闭按钮的点击事件');\n  }\n  \n  // 绑定背景点击事件\n  modal.addEventListener('click', function(e) {\n    if (e.target === modal) {\n      console.log('✓ 背景被点击');\n      window.closeAppointmentModal();\n    }\n  });\n  console.log('✓ 已绑定背景点击事件');\n  \n  // 绑定ESC键事件\n  document.addEventListener('keydown', function(e) {\n    if (e.key === 'Escape' && modal.classList.contains('active')) {\n      console.log('✓ ESC键被按下');\n      window.closeAppointmentModal();\n    }\n  });\n  console.log('✓ 已绑定ESC键事件');\n  \n  console.log('=== 初始化完成 ===');\n}\n\n// 等待DOM加载完成后初始化\nif (document.readyState === 'loading') {\n  document.addEventListener('DOMContentLoaded', initAppointmentModal);\n  console.log('⏳ 等待DOMContentLoaded事件...');\n} else {\n  initAppointmentModal();\n  console.log('✓ DOM已加载,直接初始化');\n}\n\n// 延迟检查\nsetTimeout(function() {\n  if (typeof window.openAppointmentModal !== 'function') {\n    console.error('✗ 错误: openAppointmentModal 函数未定义');\n  } else {\n    console.log('✓ 延迟检查: openAppointmentModal 函数已正确定义');\n  }\n}, 500);\n</script>"
