chInput').val('');\n                        var i, tabContent, tablinks;\n                        tabContent = document.getElementsByClassName(\"city\");\n                        for (i = 0; i < tabContent.length; i++) {\n                            tabContent[i].style.display = \"none\";\n                        }\n                        tablinks = document.getElementsByClassName(\"country\");\n                        for (i = 0; i < tablinks.length; i++) {\n                            tablinks[i].classList.remove(\"active\");\n                        }\n                        document.getElementById(tabName).style.display = \"block\";\n                        event.currentTarget.classList.add(\"active\");\n                    }\n\n                    $(document).ready(function () {\n\n                        $('#searchInput').on('keyup', function (event) {\n                            if (event.keyCode === 13) { \/\/ 检查是否按下回车键\n                                searchItems();\n                            }\n                        });\n                        $(\"#searchButton\").on('click', function () {\n                            searchItems();\n                        })\n                        function searchItems() {\n                            const filter = UIkit.filter('#salon-filter'); \/\/ 确保 UIkit filter 正确实例化\n                            const input = $('#searchInput').val().toLowerCase().trim(); \/\/ 获取用户输入并去掉多余空格\n                            console.log('input', input);\n                            \/\/ 遍历所有列表项，检查是否匹配输入内容\n                            $('.uk-list li').each(function () {\n                                const hairstyallText = $(this).find('.right p').text().toLowerCase();\n                                const hairstylistText = $(this).find('.right p strong').text().toLowerCase();\n                                const addressText = $(this).find('.right span').text().toLowerCase();\n                                const country = $(this).data('country') ? $(this).data('country').toString().toLowerCase() : '';\n                                const city = $(this).data('city') ? $(this).data('city').toString().toLowerCase() : '';\n                                console.log('country', country);\n                                console.log('country', country.includes(input));\n\n                                \/\/ 检查是否匹配用户输入\n                                const matches =\n                                    hairstyallText.includes(input) ||\n                                    hairstylistText.includes(input) ||\n                                    addressText.includes(input) ||\n                                    country.includes(input) ||\n                                    city.includes(input);\n\n                                \/\/ 显示或隐藏匹配项\n                                $(this).toggle(matches);\n                            });\n                            resetFilterButton();\n                        }\n\n                        \/\/ 去抖函数实现\n                        function debounce(func, wait) {\n                            let timeout;\n                            return function (...args) {\n                                const context = this;\n                                clearTimeout(timeout);\n                                timeout = setTimeout(() => func.apply(context, args), wait);\n                            };\n                        }\n                        function resetFilterButton() {\n                            $('.country').removeClass('active'); \/\/ 移除所有按钮的高亮状态\n                            $('.country').removeClass('uk-active');\n                            $('.sub-filter>li').hide();\n                            $('#allFilterButton').addClass('active'); \/\/ 给 \"All\" 按钮添加高亮状态\n                        }\n                    });\n                <\/script>\n<\/div>\n<\/div>\n<\/div>\n<\/div>"}}},"block_order":["html_zCXpyh"],"name":"[gy] Custom HTML Conten