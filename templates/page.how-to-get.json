end_email_input.length <= 0){\n                $('.email_erro').text('please enter your email');\n                return false;\n            }else{\n                $('.email_erro').text('');\n            }\n            if(first_email.length > 0){\n                if (!regEmail.test(first_email)){\n                    $('.first_email_erro').text('please enter the right email address');\n                    return false;\n                }else{\n                    $('.first_email_erro').text('');\n                    $.post('/frontapi/common.php',{action:'sendmail',type:'refer',email:first_email,params:'{\"tid\":\"oldfriend\",\"from_customer_id\":\"'+id_customer+'\",\"code\":\"'+friend_code+'\",\"to_name\":\"\",\"content\":\"'+filterHTMLTag(friend_textarea)+'\"}'},function(res){\n                        if(res.status == 'Y'){\n                            if(mail_num == 1){\n    \n                                $('.send_email_success').show().delay(3000).hide(0);\n    \n                            }\n                        }\n                    })\n                }\n            }\n            if(second_email.length > 0){\n                if (!regEmail.test(second_email)){\n                    $('.second_email_erro').text('please enter the right email address');\n                    return false;\n                }else{\n                    $('.second_email_erro').text('');\n                    $.post('/frontapi/common.php',{action:'sendmail',type:'refer',email:first_email,params:'{\"tid\":\"oldfriend\",\"from_customer_id\":\"'+id_customer+'\",\"code\":\"'+friend_code+'\",\"to_name\":\"\",\"content\":\"'+filterHTMLTag(friend_textarea)+'\"}'},function(res){\n                        if(res.status == 'Y'){\n                            if(mail_num == 2){\n    \n                                $('.send_email_success').show().delay(3000).hide(0);\n    \n                            }\n                        }\n                    })\n                }\n            }\n            if(third_email.length > 0){\n                if (!regEmail.test(third_email)){\n                    $('.third_email_erro').text('please enter the right email address');\n                    return false;\n                }else{\n                    $('.third_email_erro').text('');\n                    $.post('/frontapi/common.php',{action:'sendmail',type:'refer',email:first_email,params:'{\"tid\":\"oldfriend\",\"from_customer_id\":\"'+id_customer+'\",\"code\":\"'+friend_code+'\",\"to_name\":\"\",\"content\":\"'+filterHTMLTag(friend_textarea)+'\"}'},function(res){\n                        if(res.status == 'Y'){\n                            if(mail_num == 3){\n    \n                                $('.send_email_success').show().delay(3000).hide(0);\n    \n                            }\n                        }\n                    })\n                }\n            }\n    \n        };\n        function filterHTMLTag(str) {\n            var msg = str.replace(/<\\/?[^>]*>/g, ''); //去除HTML Tag\n            msg = msg.replace(/[|]*\\n/, ''); //去除行尾空格\n            msg = msg.replace(/&nbsp;/ig, ''); //去掉npsp\n            msg = msg.replace(/\"/ig, \"'\"); //去掉npsp\n            return msg;\n        }\n    </script>\n    <script>\n        $(function() {\n            $('.how-to-review').click(function() {\n                $('#iframe_how_to_review').attr('src', $('#iframe_how_to_review').attr('src-url'));\n                $('#iframe_how_to_review').contents().find('img').width('100%');\n                $('#how-to-review').addClass('in');\n                $('body').addClass('modal-open');\n            });\n            $('.refer-to-friend').click(function() {\n                $('#iframe_refer_to_friend').attr('src', $('#iframe_refer_to_friend').attr('src-url'));\n                $('#iframe_refer_to_friend').contents().find('img').width('100%');\n                $('#refer-to-friend').addClass('in');\n                $('body').addClass('modal-open');\n            });\n   